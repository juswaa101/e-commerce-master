                <img
                  src="data:image/png;base64,"
                />
                <h2>Orders</h2>
                <h6><% orders_count.forEach(element => { %> <%= element.orders_count %> <% }) %></h6>
              </a>
            </center>
          </div>
        </div>

        <div class="col-md-3">
          <div
            class="container"
            style="
              padding: 0.5em;
              border: #5765c1 solid 2px;
              border-radius: 3px;
            "
          >
            <center>
              <a
                href="/adminCompleted"
                style="text-decoration: none; color: black"
              >
                <img
                  src="data:image/png;base64,"
                />
                <h2>Completed</h2>
                <h6><% orders_compeleted_count.forEach(element => { %> <%= element.orders_completed_count %> <% }) %></h6>
              </a>
            </center>
          </div>
        </div>
      </div>

